{
  "name": "ur-money",
  "version": "1.0.0",
  "description": "A self-hosted web app to manage your money and track spending",
  "main": "server/index.js",
  "scripts": {
    "server": "node server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "npm run server"
  },
  "keywords": ["money", "finance", "budget", "self-hosted"],
  "author": "Ines Mouhouche Bideira",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "sqlite3": "^5.1.6",
    "body-parser": "^1.20.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
